{
    "hello": "world",
    "foo": [
        "a",
        // this is a c comment in a list
        "b",
        {
            "a": "b"
        }, {
            "c": "{this is a test}"
        }
    ],
    "bar": {
        "a": "b",
        // a c comment
        "d": "e",
        // another c comment
        "f": 9,
        "g": [],
        "h" : {} // this is an inline comment 
    }
}